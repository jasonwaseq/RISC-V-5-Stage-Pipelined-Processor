# iCEBreaker Pin Constraints (iCE40UP5K-SG48)

# 12 MHz clock
set_io clk  35

# Reset button (directly active-low to GPIO BTN_N)
set_io rst_n 10

# LEDs (accent LEDs on iCEBreaker)
set_io led[0] 11
set_io led[1] 37
set_io led[2] 38  # Red
set_io led[3] 39  # Green
set_io led[4] 40  # Blue

# UART (directly to FTDI chip)
set_io uart_rx 6
set_io uart_tx 9
